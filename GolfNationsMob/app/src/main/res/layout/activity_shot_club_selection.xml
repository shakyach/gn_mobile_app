<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.golfnationsmob.GolfView.NewRound.ShotClubSelection"
    android:background="#ffffff">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:fillViewport="false"
        android:scrollbars="none"
        android:layout_marginBottom="60dp">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ebedf1"
        android:id="@+id/linearLayout18">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="WOODS"
            android:id="@+id/textView40"
            android:layout_marginLeft="15dp"
            android:layout_gravity="center_vertical"
            android:textColor="#10171F"
            android:textSize="14dp"
            android:fontFamily="font/Roboto-Regular_0.ttf" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeSC1"
        android:layout_below="@+id/linearLayout18"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:scrollbars="none"
        android:layout_marginTop="20dp" >



            <Button
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:text="Dr"
                android:id="@+id/buttonCSw0"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:textColor="#a5a6a7"
                android:textSize="15dp"
                android:background="@drawable/whitebox"
                android:visibility="gone" />

            <Button
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:text="3w"
                android:id="@+id/buttonCSw1"
                android:background="@drawable/whitebox"
                android:textColor="#a5a6a7"
                android:textSize="15dp"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/buttonCSw0"
                android:layout_toEndOf="@+id/buttonCSw0"
                android:layout_marginLeft="20dp"
                android:visibility="gone" />

            <Button
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:text="5w"
                android:id="@+id/buttonCSw2"
                android:background="@drawable/whitebox"
                android:textColor="#a5a6a7"
                android:textSize="15dp"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/buttonCSw1"
                android:layout_toEndOf="@+id/buttonCSw1"
                android:layout_marginLeft="20dp"
                android:visibility="gone" />
    </RelativeLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/horizontalScrollView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:background="#ebedf1"
        android:id="@+id/linearLayout19">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="HYBRID"
            android:id="@+id/textView41"
            android:layout_marginLeft="15dp"
            android:layout_gravity="center_vertical"
            android:textColor="#10171F"
            android:textSize="14dp"
            android:fontFamily="font/Roboto-Regular_0.ttf" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeSC2"
        android:layout_below="@+id/linearLayout19"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:scrollbars="none"
        android:layout_marginTop="20dp" >


    </RelativeLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:scrollbars="none"
            android:background="#ebedf1"
            android:id="@+id/linearLayout188"
            android:layout_marginTop="20dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="IRON"
                android:id="@+id/textView400"
                android:layout_marginLeft="15dp"
                android:layout_gravity="center_vertical"
                android:textColor="#10171F"
                android:textSize="14dp"
                android:fontFamily="font/Roboto-Regular_0.ttf" />
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/relativeSC3"
            android:layout_below="@+id/linearLayout188"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:scrollbars="none"
            android:layout_marginTop="20dp" >


        </RelativeLayout>
    </LinearLayout>
    </ScrollView>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:weightSum="2"
        android:background="#10171f"
        android:id="@+id/linear34444">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="MY CLUBS "
            android:id="@+id/idmyclub"
            android:background="#10171f"
            android:textColor="#ffffff"

            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textSize="15dp"
            android:layout_weight="1"
            android:textAlignment="center"
            android:layout_marginRight="120dp"
            android:fontFamily="font/Roboto-Regular_0.ttf" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="SAVE"
            android:id="@+id/idSCSsave"
            android:layout_alignTop="@+id/idmyclub"
            android:layout_toRightOf="@+id/idmyclub"
            android:layout_toEndOf="@+id/idmyclub"
            android:background="#10171f"
            android:textColor="#ffffff"
            android:textSize="15dp"
            android:layout_weight="1"
            android:textAlignment="center"
            android:layout_marginLeft="15dp"
            android:fontFamily="font/Roboto-Regular_0.ttf" />
    </LinearLayout>
</RelativeLayout>
